<?xml version="1.0" encoding="Windows-1252"?>
<VisualStudioPropertySheet
	ProjectType="Visual C++"
	Version="8.00"
	Name="znet_projectsd"
	OutputDirectory="$(ZNET_BIN)"
	IntermediateDirectory="$(ProjectName)_$(ConfigurationName)"
	>
	<Tool
		Name="VCCLCompilerTool"
		AdditionalIncludeDirectories="&quot;$(PROJECTS_INC)&quot;
						;&quot;$(ZNET_INC)&quot;
						;&quot;$(ACE_INC)&quot;
						;&quot;$(TAO_ROOT)&quot;
						;&quot;$(TAO_ROOT)/orbsvcs&quot;
						;&quot;$(GSL_INC)&quot;;"
		PreprocessorDefinitions="$(CUSTOM_DEFINES)"
		WarningLevel="0"
	/>
	<Tool
		Name="VCLinkerTool"
		OutputFile="$(OutDir)/$(ProjectName)$(BUILD_SFX).exe"
		AdditionalDependencies=" znet$(BUILD_SFX).lib ace$(BUILD_SFX).lib TAO$(BUILD_SFX).lib TAO_IORTable$(BUILD_SFX).lib TAO_SmartProxies$(BUILD_SFX).lib TAO_AnyTypeCode$(BUILD_SFX).lib TAO_CosNaming$(BUILD_SFX).lib TAO_PortableServer$(BUILD_SFX).lib TAO_CosNaming_Skel$(BUILD_SFX).lib TAO_Valuetype$(BUILD_SFX).lib TAO_CodecFactory$(BUILD_SFX).lib TAO_PI$(BUILD_SFX).lib TAO_Messaging$(BUILD_SFX).lib TAO_Svc_Utils$(BUILD_SFX).lib TAO_CosNaming_Serv$(BUILD_SFX).lib"
		AdditionalLibraryDirectories="&quot;$(ZNET_LIB)&quot;
						;&quot;$(PROJECTS_LIB)&quot;
						;&quot;$(PROJECTS_BIN)&quot;
						;&quot;$(ACE_LIB)&quot;
						;&quot;$(GSL_LIB)&quot;;"
		ProgramDatabaseFile="$(OutDir)/$(ProjectName)$(BUILD_SFX).pdb"
		ImportLibrary="$(PROJECTS_LIB)/$(ProjectName)$(BUILD_SFX).lib"
	/>
	<UserMacro
		Name="BUILD_SFX"
		Value="d"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="BUILD_MODE"
		Value="$(ConfigurationName)"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="ACE_ROOT"
		Value="$(VLIB_ROOT)\ACE_wrappers"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="ACE_INC"
		Value="$(ACE_ROOT)"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
