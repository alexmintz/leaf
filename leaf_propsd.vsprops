		Value="$(ACE_ROOT)\lib"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="ACE_BIN"
		Value="$(ACE_ROOT)\bin"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="TAO_ROOT"
		Value="$(ACE_ROOT)\TAO"
		PerformEnvironmentSet="true"
	/>
	<UserMacro
		Name="SQLAPI_ROOT"
		Value="$(VLIB_ROOT)\SQLAPI"
